{
    "name": "atmaliance/yii2-atm-widget",
    "description": "ATM Widget",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","atm72","widget"],
    "license": "MIT",
    "authors": [
        {
            "name": "Vitaliy Popov",
            "email": "popov.v.o@atm72.ru",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "yiisoft/yii2": "2.*",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-bootstrap4": "~2.0.8",
        "atmaliance/yii2-atm-asset": "dev-master"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "suggest": {
        "rmrevin/yii2-fontawesome": "Font icons"
    },
    "autoload": {
        "psr-4": {
            "atmaliance\\yii2_atm_widget\\": "src"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
